{
  "name": "ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "dodo": " DEBUG=*  node -r dotenv/config ./bin/dodo dotenv_config_path=.env ",
    "start": "node dist/bin/server.js",
    "watch": "nodemon ./dist/bin/server.js",
    "ts-build": "tsc",
    "start:dev": "node  -r dotenv/config farm dotenv_config_path=.env.dev",
    "copy-static": "ts-node copyStatic.ts",
    "build": "npm run ts-build && npm run copy-static"
  },
  "dependencies": {
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-jq": "^1.11.3",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2"
  }
}
